{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "basic-ADF"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"AzureSqlDatabase2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase2'"
		},
		"FileServer1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'FileServer1'"
		},
		"FileServer1_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\Users\\Akhilesh\\Downloads\\CASE_sTUDY\\DATA"
		},
		"FileServer1_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "LAPTOP-JL8DML4Q\\Akhilesh"
		},
		"ExceltoDBrefresh_properties_BulkCopyfromExceltoSQL_parameters_sheetList": {
			"type": "array",
			"defaultValue": [
				"Vendor",
				"Date",
				"Category",
				"DefectType",
				"Plant",
				"Metrics",
				"MaterialType",
				"Defect"
			]
		},
		"ExceltoDBrefresh_properties_CopyDatafromDBtoBackupDB_parameters_list": {
			"type": "array",
			"defaultValue": [
				"Vendor",
				"Date",
				"Category",
				"DefectType",
				"Plant",
				"Metrics",
				"MaterialType",
				"Defect"
			]
		},
		"DBtobackupDBrefresh_properties_CopyDatafromDBtoBackupDB_parameters_list": {
			"type": "array",
			"defaultValue": [
				"Vendor",
				"Date",
				"Category",
				"DefectType",
				"Plant",
				"Metrics",
				"MaterialType",
				"Defect"
			]
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "basic-ADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"AzureSqlDatabase2_connectionString": {
						"value": "[parameters('AzureSqlDatabase2_connectionString')]"
					},
					"FileServer1_password": {
						"value": "[parameters('FileServer1_password')]"
					},
					"FileServer1_properties_typeProperties_host": {
						"value": "[parameters('FileServer1_properties_typeProperties_host')]"
					},
					"FileServer1_properties_typeProperties_userId": {
						"value": "[parameters('FileServer1_properties_typeProperties_userId')]"
					},
					"DBtobackupDBrefresh_properties_CopyDatafromDBtoBackupDB_parameters_list": {
						"value": "[parameters('DBtobackupDBrefresh_properties_CopyDatafromDBtoBackupDB_parameters_list')]"
					},
					"ExceltoDBrefresh_properties_BulkCopyfromExceltoSQL_parameters_sheetList": {
						"value": "[parameters('ExceltoDBrefresh_properties_BulkCopyfromExceltoSQL_parameters_sheetList')]"
					},
					"ExceltoDBrefresh_properties_CopyDatafromDBtoBackupDB_parameters_list": {
						"value": "[parameters('ExceltoDBrefresh_properties_CopyDatafromDBtoBackupDB_parameters_list')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}